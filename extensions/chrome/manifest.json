{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version" : 2,
    "name" : "Coagmento Problem Help Study",
    "description" : "Extension for Problem Help study for Coagmento.",
    "version" : "1.0.7",
    "content_scripts": [],
    "background" : {
        "persistent": false,
        "scripts" : ["background.js"]
    },
    "web_accessible_resources":["*.png"],
    
    "homepage_url":"http://new.coagmento.org/",
    "permissions" : [
        "history",
        "storage",
        "tabs",
        "<all_urls>",
        "webNavigation",
		"contextMenus"
    ],
    "omnibox": { "keyword" : "test" },
    
    "browser_action": {
        "default_icon": "icons/logo-16.png",
        "default_popup": "login.html",
        "default_title": "Problem Help Study Extension"
    },
    
    "icons":{
        "48": "icons/logo-48.png",
        "16": "icons/logo-16.png"
    },
    
    "minimum_chrome_version" : "35"
}
