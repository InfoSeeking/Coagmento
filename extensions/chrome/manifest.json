{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version" : 2,
    "name" : "Coagmento Problem Help Study",
    "description" : "Extension for Problem Help study for Coagmento.",
    "version" : "1.0.14",
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["contentScript.js"]
      }
    ],
    "background" : {
        "persistent": false,
        "scripts" : ["config.js","background.js"]
    },
    "web_accessible_resources":["*.png"],

    "homepage_url":"http://new.coagmento.org/",
    "permissions" : [
        "history",
        "storage",
        "tabs",
        "activeTab",
        "<all_urls>",
        "webNavigation",
	    	"contextMenus",
        "notifications",
        "*://*/*"
    ],
    "omnibox": { "keyword" : "test" },

    "browser_action": {
        "default_icon": "icons/logo-16.png",
        "default_popup": "loggedin.html",
        "default_title": "Problem Help Study Extension"
    },

    "icons":{
        "48": "icons/logo-48.png",
        "16": "icons/logo-16.png"
    },

    "minimum_chrome_version" : "35"
}
